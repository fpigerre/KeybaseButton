{
    "name": "KeybaseButton",
    "version": "1.0",
    "manifest_version" : 2,
    "description": "Keybase button for GitHub profiles",
    "content_scripts": [
        {
            "matches": ["https://github.com/*", "https://www.github.com/*"],
            "js": ["scripts/jquery-2.0.0.min.js","scripts/main.js"],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "16": "icons/16.png"
    },
    "permissions": [
        "https://github.com/*",
        "https://www.github.com/*",
        "https://keybase.io/*"
    ]
}
